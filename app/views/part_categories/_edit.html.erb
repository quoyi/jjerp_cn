<!-- Modal -->
<div class="modal fade" id="editPartCategory" tabindex="-1" role="dialog" aria-labelledby="editPartCategoryLabel">
  <div class="modal-dialog modal-sm" role="document">
    <div class="modal-content">
      <form action="" id="part_category_form">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
          <h4 class="modal-title" id="editPartCategoryLabel"> 配件类型 <small>编辑</small></h4>
        </div>
        <div class="modal-body">
          <input type="hidden" id="part_category_id" name="id">
          <div class="form-group">
            <div class="row">
              <div class="col-sm-12">
                <label for="part_category_name" class="control-label col-sm-3 text-center" style="padding: 6px 0;">名称</label>
                <div class="col-sm-9">
                  <input type="text" name="name" id="part_category_name">
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button class="btn btn-default" data-dismiss="modal">取消</button>
          <button type="submit" class="btn btn-primary" id="part_category_submit">提交</button>
        </div>
      </form>
    </div>
  </div>
